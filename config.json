{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 20,
        "sync": "batch"
    },
    "sync_batcher": {
        "interval": "3s",
        "max_pending": 10
    },
    "bssplit": "10MB/100/pak",
    "iosize": "128KB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
