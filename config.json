{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 10,
        "sync": "inline",
        "sync_on": "close",
        "open_flags": ["O_SYNC"]
    },
    "sync_batcher": {
        "interval": "2s",
        "max_pending": 5
    },
    "size": "5MB",
    "iosize": "256KB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
