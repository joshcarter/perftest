{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 10,
        "sync": "batch",
        "sync_on": "close"
    },
    "sync_batcher": {
        "interval": "2s",
        "max_pending": 5
    },
    "size": "5MB",
    "iosize": "256KB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
