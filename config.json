{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 10,
        "sync": "close"
    },
    "sync_batcher": {
        "interval": "3s",
        "max_pending": 3
    },
    "bssplit": "100MB/100/pak",
    "iosize": "1MB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
