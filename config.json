{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 10,
        "sync": "batch",
        "sync_on": "close"
    },
    "sync_batcher": {
        "max_wait": "100ms",
        "max_pending": 15,
        "parallel": true
    },
    "size": "4MB",
    "iosize": "1MB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
