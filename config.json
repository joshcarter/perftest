{
    "file": {
        "paths": ["/tmp/perftest"],
        "runners_per_path": 20,
        "sync": "batch",
        "sync_on": "close"
    },
    "sync_batcher": {
        "interval": "3s",
        "max_pending": 10
    },
    "bssplit": "5MB/100/dat",
    "iosize": "256KB",
    "reporter": {
        "interval": "1s",
        "logbandwidth": false,
        "loglatency": false
    }
}
